;;;; Create SRFI repositories on Github
;;;;
;;;; based on download from srfi.schemers.org created using
;;;;   <wget --mirror srfi.schemers.org>

(load-option 'format)

;; Set this manually.
(define github-authorization-token #f)

(define github-api-srfi-repos
  "https://api.github.com/orgs/scheme-requests-for-implementation/repos")

(define-record-type srfi
    (%make-srfi number status title authors draft-date final-date)
    srfi?
  (number     srfi/number)
  (status     srfi/status)
  (title      srfi/title)
  (authors    srfi/authors)
  (draft-date srfi/draft-date)
  (final-date srfi/final-date))

(define (make-srfi number status title authors draft-date #!optional final-date)
  (%make-srfi number
	      status
	      title
	      authors
	      draft-date
	      (and (not (default-object? final-date))
		   final-date)))

